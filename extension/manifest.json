{
	"name": "Overlay Tips",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Overlay informations on the web page",
    "permissions": [
        "http://192.168.33.10/"
    ],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
    "content_scripts": [
        {
            "matches": ["http://www.elasticsearch.org/*"],
            "css": [],
            "js": ["jquery-2.1.0.min.js", "myscript.js"]
        }
    ]
}
